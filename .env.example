# Environment variables for Cropwatch application
# Copy this file to .env and fill in your actual values

# NASA Earthdata token for accessing GIBS/HLS data
EARTHDATA_TOKEN=your_earthdata_token_here

# HuggingFace tokens for accessing private spaces
HUGGINGFACEHUB_API_TOKEN=your_huggingface_token_here
HF_TOKEN=your_huggingface_token_here

# Optional: Set custom port (default: 8000)
# PORT=8000

# Optional: Set custom host (default: 0.0.0.0 in Docker)
# HOST=0.0.0.0

# Optional: Enable debug mode (development only)
# DEBUG=false